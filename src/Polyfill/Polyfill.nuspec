<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Polyfill</id>
    <version>$version$</version>
    <authors>$authors$</authors>
    <developmentDependency>true</developmentDependency>
    <license type="expression">MIT</license>
    <projectUrl>https://github.com/SimonCropp/Polyfill</projectUrl>
    <icon>icon.png</icon>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>$description$</description>
    <copyright>$copyright$</copyright>
    <tags>$tags$</tags>
  </metadata>
  <files>
    <file src="../icon.png" target="icon.png" />
    <file src="*.cs"
          target="contentFiles/cs/netstandard2.0/Polyfill/"/>
    <file src="Nullable\*.cs"
          target="contentFiles/cs/netstandard2.0/Polyfill/Nullable"/>
    <file src="Nullability\*.cs"
          target="contentFiles/cs/netstandard2.0/Polyfill/Nullability"/>
    <file src="IndexRange\*.cs"
          target="contentFiles/cs/netstandard2.0/Polyfill/IndexRange"/>
    <file src="Trimming\*.cs"
          target="contentFiles/cs/netstandard2.0/Polyfill/Trimming"/>
    <file src="PlatformCompatibility\*.cs"
          target="contentFiles/cs/netstandard2.0/Polyfill/PlatformCompatibility"/>
    <file src="Polyfill.props"
          target="build"/>
    <file src="Polyfill.targets"
          target="build"/>
  </files>
</package>
